<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/">
<ShortName>Google Play (日本語) - 有料</ShortName>
<Description>Google Play 日本語 にてSSL通信による検索を行います</Description>
<InputEncoding>UTF-8</InputEncoding>
<Image width="16" height="16">data:image/png;base64,</Image>
<SearchForm>https://play.google.com</SearchForm>
<Url type="text/html" method="GET" template="https://play.google.com/store/search">
  <Param name="hl" value="ja"/>
  <Param name="c" value="apps"/>
  <Param name="q" value="{searchTerms}"/>
   <Param name="price" value="2"/>
</Url>
<Url type="application/x-suggestions+json" method="GET" template="https://play.google.com/store/search?q={searchTerms}&amp;c=apps&amp;hl={moz:locale}">
</Url>
</SearchPlugin>
